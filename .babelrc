{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "chrome": 49,
          "firefox": 45,
          "safari": 10,
          "edge": 13,
          "ie": 11
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {               
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": false,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}
